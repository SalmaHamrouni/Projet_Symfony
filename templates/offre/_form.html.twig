{% block stylesheets %}
    <link rel="stylesheet" href="{{asset('assets/styleForm.css')}}">
{% endblock %}

{{ form_start(form) }}
<div class="container">
    <div class="form-group"> 
        {{ form_row(form.Contenu, {'attr': {'class': 'form-input' }}) }}
        </div>
    <div class="form-group"> 
        {{ form_row(form.Categorie, {'attr': {'class': 'form-input' }}) }}
    </div>
    <div class="form-group"> 
        {{ form_widget(form.Date_C, {'attr': {'type': "date", 'class': 'form-input' }}) }}
        
    </div>
    <button class="btn">{{ button_label|default('Save') }}</button>

{{ form_end(form) }}
