{% block stylesheets %}
    <link rel="stylesheet" href="{{asset('assets/styleForm.css')}}">
{% endblock %}
{{ form_start(form) }}
<div class="grix xs4 container">
		<div class="col-xs4">
  {{ form_row(form.name, {'attr': {'class': 'form-input' }}) }}
  
    <div class="form-group"> 
        {{ form_row(form.name, {'attr': {'class': 'form-input' }}) }}
    </div>
    <div class="form-group"> 
        {{ form_row(form.email, {'attr': {'class': 'form-input' }}) }}
    </div>
    <div class="form-group"> 
        {{ form_row(form.password, {'attr': {'class': 'form-input' }}) }}
    </div>
    <div class="form-group"> 
        {{ form_row(form.CIN, {'attr': {'class': 'form-input' }}) }}
        </div>
  
  
</div> 
    <button class="btn btn-success">{{ button_label|default('Save') }}</button>

</div>  
  

{{ form_end(form) }}
