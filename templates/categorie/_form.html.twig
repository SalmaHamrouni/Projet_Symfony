{% block stylesheets %}
    <link rel="stylesheet" href="{{asset('assets/styleForm.css')}}">
{% endblock %}
{{ form_start(form) }}
    <div class="form-group"> 
        {{ form_widget(form.titre, {'attr': {'class': 'form-input'}}) }}
    </div>
    <div class="form-group"> 
        {{ form_widget(form.description, {'attr': {'class': 'form-input'}}) }}
    </div>
    <div class="form-group"> 
        <button class="btn btn-success"">{{ button_label|default('Save') }}</button>
        </div>
{{ form_end(form) }}
