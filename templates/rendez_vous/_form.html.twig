{% block stylesheets %}
    <link rel="stylesheet" href="{{asset('assets/styleForm.css')}}">
{% endblock %}
{{ form_start(form) }}
<div class="container">
    <div class="form-group"> 
        Adresse {{ form_widget(form.adresse, {'attr': {'class': 'form-input' }}) }}
    </div>
    <div class="form-group"> 
        Date {{ form_widget(form.Date, {'attr': {'class': 'form-input' }}) }}
    </div>
    <div class="form-group"> 
        Heure {{ form_widget(form.heure, {'attr': {'class': 'form-input' }}) }}
    </div>
    <div class="form-group"> 
        A noter{{ form_widget(form.remarque, {'attr': {'class': 'form-input' }}) }}
    </div>

    <button class="btn btn-success">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
